{
	"name": "objecs",
	"version": "0.0.14",
	"description": "ECS",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist",
		"src"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/jakeklassen/objecs.git"
	},
	"scripts": {
		"postdocs": "cp jekyll.config.yml docs/config.yml && touch docs/.nojekyll",
		"lint": "eslint src --ext .js,.ts --config .eslintrc.cjs",
		"lint:fix": "eslint src --ext .js,.ts --config .eslintrc.cjs --fix",
		"prebuild": "npm run clean",
		"build": "npm run lint && tsup src --format cjs,esm --dts-resolve",
		"build:watch": "nodemon --exec 'npm run build || exit 1'",
		"prepublishOnly": "nr build",
		"release": "bumpp --commit --push --tag && pnpm publish",
		"test": "vitest",
		"pretest:coverage": "npm run clean",
		"test:coverage": "vitest run --coverage",
		"clean": "rimraf dist"
	},
	"author": "Jake Klassen <jklassendev@gmail.com>",
	"license": "MIT",
	"keywords": [
		"gamedev",
		"ecs",
		"entity component system",
		"typescript"
	],
	"devDependencies": {
		"@antfu/ni": "0.21.5",
		"@types/benchmark": "^2.1.2",
		"@types/eslint": "^8.44.0",
		"@types/node": "^20.4.4",
		"@typescript-eslint/eslint-plugin": "^6.1.0",
		"@typescript-eslint/parser": "^6.1.0",
		"benchmark": "^2.1.4",
		"bumpp": "9.1.1",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "5.0.0",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.0",
		"rimraf": "^5.0.1",
		"typescript": "^5.1.6",
		"vite": "4.4.6",
		"vitest": "0.33.0"
	},
	"dependencies": {
		"tsup": "^7.1.0",
		"type-fest": "4.0.0"
	}
}

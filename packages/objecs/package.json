{
	"name": "objecs",
	"version": "0.0.29",
	"description": "ECS",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist",
		"src"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/jakeklassen/objecs.git"
	},
	"scripts": {
		"postdocs": "cp jekyll.config.yml docs/config.yml && touch docs/.nojekyll",
		"lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
		"lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
		"prebuild": "pnpm run clean",
		"build": "pnpm run lint && tsup",
		"build:watch": "nodemon --exec 'pnpm run build || exit 1'",
		"prepublishOnly": "nr build",
		"release": "bumpp --commit --push --tag && pnpm publish",
		"test": "vitest",
		"pretest:coverage": "pnpm run clean",
		"test:coverage": "vitest run --coverage",
		"clean": "rimraf dist",
		"check-exports": "attw --pack ."
	},
	"author": "Jake Klassen <jklassendev@gmail.com>",
	"license": "MIT",
	"keywords": [
		"gamedev",
		"ecs",
		"entity component system",
		"typescript"
	],
	"devDependencies": {
		"@antfu/ni": "28.0.0",
		"@arethetypeswrong/cli": "0.18.2",
		"@eslint/eslintrc": "3.3.3",
		"@eslint/js": "9.39.2",
		"@types/benchmark": "^2.1.5",
		"@types/eslint": "^9.6.1",
		"@types/node": "^24.10.4",
		"@typescript-eslint/eslint-plugin": "^8.50.0",
		"@typescript-eslint/parser": "^8.50.0",
		"benchmark": "^2.1.4",
		"bumpp": "10.3.2",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-depend": "1.4.0",
		"eslint-plugin-prettier": "5.5.4",
		"globals": "16.5.0",
		"nodemon": "^3.1.11",
		"prettier": "^3.7.4",
		"rimraf": "^6.1.2",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"vite": "7.3.0",
		"vitest": "4.0.16"
	},
	"dependencies": {
		"type-fest": "5.3.1"
	}
}

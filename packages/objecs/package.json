{
	"name": "objecs",
	"version": "0.0.25",
	"description": "ECS",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist",
		"src"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/jakeklassen/objecs.git"
	},
	"scripts": {
		"postdocs": "cp jekyll.config.yml docs/config.yml && touch docs/.nojekyll",
		"lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" eslint.config.mjs",
		"lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\" eslint.config.mjs",
		"prebuild": "npm run clean",
		"build": "npm run lint && tsup",
		"build:watch": "nodemon --exec 'npm run build || exit 1'",
		"prepublishOnly": "nr build",
		"release": "bumpp --commit --push --tag && pnpm publish",
		"test": "vitest",
		"pretest:coverage": "npm run clean",
		"test:coverage": "vitest run --coverage",
		"clean": "rimraf dist",
		"check-exports": "attw --pack ."
	},
	"author": "Jake Klassen <jklassendev@gmail.com>",
	"license": "MIT",
	"keywords": [
		"gamedev",
		"ecs",
		"entity component system",
		"typescript"
	],
	"devDependencies": {
		"@antfu/ni": "0.23.0",
		"@arethetypeswrong/cli": "0.16.4",
		"@eslint/js": "9.13.0",
		"@types/benchmark": "^2.1.5",
		"@types/eslint": "^9.6.1",
		"@types/eslint__eslintrc": "2.1.2",
		"@types/eslint__js": "8.42.3",
		"@types/node": "^22.8.4",
		"@typescript-eslint/eslint-plugin": "^8.12.2",
		"@typescript-eslint/parser": "^8.12.2",
		"benchmark": "^2.1.4",
		"bumpp": "9.8.0",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-depend": "0.11.0",
		"eslint-plugin-prettier": "5.2.1",
		"globals": "15.11.0",
		"nodemon": "^3.1.7",
		"prettier": "^3.3.3",
		"rimraf": "^6.0.1",
		"tsup": "^8.3.5",
		"typescript": "^5.6.3",
		"vite": "5.4.10",
		"vitest": "2.1.4"
	},
	"dependencies": {
		"type-fest": "4.26.1"
	}
}

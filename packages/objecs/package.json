{
	"name": "objecs",
	"version": "0.0.26",
	"description": "ECS",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist",
		"src"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/jakeklassen/objecs.git"
	},
	"scripts": {
		"postdocs": "cp jekyll.config.yml docs/config.yml && touch docs/.nojekyll",
		"lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" eslint.config.mjs",
		"lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\" eslint.config.mjs",
		"prebuild": "npm run clean",
		"build": "npm run lint && tsup",
		"build:watch": "nodemon --exec 'npm run build || exit 1'",
		"prepublishOnly": "nr build",
		"release": "bumpp --commit --push --tag && pnpm publish",
		"test": "vitest",
		"pretest:coverage": "npm run clean",
		"test:coverage": "vitest run --coverage",
		"clean": "rimraf dist",
		"check-exports": "attw --pack ."
	},
	"author": "Jake Klassen <jklassendev@gmail.com>",
	"license": "MIT",
	"keywords": [
		"gamedev",
		"ecs",
		"entity component system",
		"typescript"
	],
	"devDependencies": {
		"@antfu/ni": "24.2.0",
		"@arethetypeswrong/cli": "0.17.4",
		"@eslint/eslintrc": "3.3.0",
		"@eslint/js": "9.22.0",
		"@types/benchmark": "^2.1.5",
		"@types/eslint": "^9.6.1",
		"@types/node": "^22.13.10",
		"@typescript-eslint/eslint-plugin": "^8.26.1",
		"@typescript-eslint/parser": "^8.26.1",
		"benchmark": "^2.1.4",
		"bumpp": "10.1.0",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-depend": "0.12.0",
		"eslint-plugin-prettier": "5.2.3",
		"globals": "16.0.0",
		"nodemon": "^3.1.9",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"tsup": "^8.4.0",
		"typescript": "^5.8.2",
		"vite": "6.2.2",
		"vitest": "3.0.8"
	},
	"dependencies": {
		"type-fest": "4.37.0"
	}
}
